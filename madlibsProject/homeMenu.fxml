<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="madlibsProject.homeMenu">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <StackPane prefHeight="376.0" prefWidth="438.0" />
      <AnchorPane prefHeight="376.0" prefWidth="438.0">
         <children>
            <Text fill="#0948ed" layoutX="55.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Madlibs">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Button fx:id="startGame" layoutX="180.0" layoutY="153.0" mnemonicParsing="false" onAction="#madlibsStoryOptions" text=" Start Game" />
            <Button fx:id="aboutInstructions" layoutX="181.0" layoutY="196.0" mnemonicParsing="false" onAction="#showInstructions" text="Instructions" />
            <Button fx:id="aboutHelp" layoutX="199.0" layoutY="234.0" mnemonicParsing="false" onAction="#helpMessage" text="Help" />
            <Button fx:id="quit" layoutX="201.0" layoutY="305.0" mnemonicParsing="false" onAction="#closeWindow" text="Quit" />
            <Button fx:id="openFiles" layoutX="185.0" layoutY="272.0" mnemonicParsing="false" onAction="#showFileOpeningSpace" text="Open Files" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
